    <!-- Way Points-->
    <script>
        function LoadBestRouteWayPoints(response) {
            document.getElementById("routeLatList").value = response.latList;
            document.getElementById("routeLngList").value = response.lngList;
            document.getElementById("routeDistance").value = response.routeDistance;

            //alert("im init map");
            var latlng = new google.maps.LatLng(7.291418, 80.636696);
            var directionsService = new google.maps.DirectionsService;
            var directionsDisplay = new google.maps.DirectionsRenderer;

            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 6,
                center: latlng
            });
            directionsDisplay.setMap(map);

            //document.getElementById('submit').addEventListener('click', function () {
            calculateAndDisplayRoute(directionsService, directionsDisplay, response);
        }

        function calculateAndDisplayRoute(directionsService, directionsDisplay, response) {
            var waypts = [];

  
            for (var i = 1; i < response.addressList.length -1; i++) {
                waypts.push({
                    location: response.addressList[i],
                    stopover: true
                });
            }


            directionsService.route({
                origin: response.addressList[0],
                destination: response.addressList[response.addressList.length - 1],
                waypoints: waypts,
                optimizeWaypoints: true,
                travelMode: 'DRIVING'
            }, function (response, status) {


                if (status === 'OK') {
                    directionsDisplay.setDirections(response);
                    //var route = response.routes[0];

                } else {
                    window.alert('Directions request failed due to ' + status);
                }
            });
        }
   </script>

