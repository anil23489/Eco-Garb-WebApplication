    <script>
        function AssignTrip() {

            var routeLatList = document.getElementById("routeLatList").value;
            var routeLngList = document.getElementById("routeLngList").value;
            var routeDistance = document.getElementById("routeDistance").value;
  
            if (routeLatList.length <= 0) {

                $('#error-modal-msg').html("Please find the best route...");
                $('#errorModal').modal({
                    show: 'show'
                });
            } else {

                $.ajax({
                    type: 'GET',
                    url: '/RoutingAlgo/AddNewRoute',
                    data: { routeLatList: routeLatList, routeLngList: routeLngList, routeDistance: routeDistance },
                    traditional: true,
                    success: function (response) {
                        $('#success-modal-msg').html("Route has been assigned successfully..");
                        $('#successModal').modal({
                            show: 'show'
                        });

                    }
                });
            }

        }
    </script>

